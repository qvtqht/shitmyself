SELECT
	vote_value,
	vote_count
FROM (
	SELECT
		vote_value,
		COUNT(vote_value) AS vote_count
	FROM
		vote
		LEFT JOIN vote_value ON (vote.vote_value = vote_value.vote)
	WHERE
		vote_value.vote_value > 0
			AND
		file_hash IN (SELECT file_hash FROM item)
	GROUP BY
		vote.vote_value,
		file_hash
	)
WHERE
	vote_count >= 1
ORDER BY
	vote_count

